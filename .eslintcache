}\n            </form>\n            <span onClick={toggleAccount}>{newAccount ? \"Sign In\" : \"Create Account\"}</span>\n            <div>\n                <button onClick={onSocialClick} name=\"google\">Continue with Google</button>\n                <button onClick={onSocialClick} name=\"github\">Continue with Github</button>\n            </div>\n        </div>\n    )\n};\n\nexport default Auth;","/Users/yurako/Documents/nwitter_ko/nacebook/src/components/Nacebook.js",["83"],"/Users/yurako/Documents/nwitter_ko/nacebook/src/routes/Profile.js",["84","85","86","87"],"/Users/yurako/Documents/nwitter_ko/nacebook/src/components/NacebookFactory.js",["88"],{"ruleId":"89","severity":1,"message":"90","line":4,"column":8,"nodeType":"91","messageId":"92","endLine":4,"endColumn":13},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","replacedBy":"96"},{"ruleId":"89","severity":1,"message":"97","line":50,"column":15,"nodeType":"91","messageId":"92","endLine":50,"endColumn":19},{"ruleId":"89","severity":1,"message":"90","line":4,"column":8,"nodeType":"91","messageId":"92","endLine":4,"endColumn":13},{"ruleId":"93","replacedBy":"98"},{"ruleId":"95","replacedBy":"99"},{"ruleId":"89","severity":1,"message":"97","line":50,"column":15,"nodeType":"91","messageId":"92","endLine":50,"endColumn":19},{"ruleId":"100","severity":1,"message":"101","line":55,"column":56,"nodeType":"102","endLine":55,"endColumn":121},{"ruleId":"89","severity":1,"message":"103","line":2,"column":17,"nodeType":"91","messageId":"92","endLine":2,"endColumn":25},{"ruleId":"89","severity":1,"message":"104","line":8,"column":12,"nodeType":"91","messageId":"92","endLine":8,"endColumn":22},{"ruleId":"89","severity":1,"message":"105","line":8,"column":24,"nodeType":"91","messageId":"92","endLine":8,"endColumn":37},{"ruleId":"106","severity":1,"message":"107","line":34,"column":8,"nodeType":"108","endLine":34,"endColumn":10,"suggestions":"109"},{"ruleId":"100","severity":1,"message":"101","line":61,"column":21,"nodeType":"